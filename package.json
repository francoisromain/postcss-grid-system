{
  "name": "francoisromain.github.io.postcss-grid-system",
  "version": "0.1.0",
  "description": "gh-pages for postcss-grid-system",
  "author": "Francois Romain <francoisromain@gmail.com>",
  "license": "MIT",
  "repository": "francoisromain/postcss-grid-system",
  "scripts": {
    "css": "postcss src/grid-system.css -o dist/grid-system.css",
    "css-00": "postcss test/src/00.css -o test/dist/00.css",
    "css-01": "postcss test/src/01.css -o test/dist/01.css",
    "css-02": "postcss test/src/02.css -o test/dist/02.css",
    "css-03": "postcss test/src/03.css -o test/dist/03.css",
    "css-05": "postcss test/src/05.css -o test/dist/05.css",
    "css-06": "postcss test/src/06.css -o test/dist/06.css",
    "css-07": "postcss test/src/07.css -o test/dist/07.css",
    "test": "npm run css && npm run css-00 && npm run css-01 && npm run css-02 && npm run css-03&& npm run css-05 && npm run css-06 && npm run css-07",
    "dev": "onchange 'index.html' 'src/**/*.*' 'postcss-grid-system/src/**/*.*' -- npm run css & livereload"
  },
  "devDependencies": {
    "livereload": "^0.9.1",
    "onchange": "^7.1.0",
    "postcss": "^8.2.2",
    "postcss-cli": "^8.3.1"
  }
}
