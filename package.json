{
  "name": "francoisromain.github.io.postcss-structure",
  "version": "0.1.0",
  "scripts": {
    "babel": "babel postcss-structure/src --out-dir postcss-structure/lib",
    "css": "postcss --local-plugins -u postcss-import -u postcss-custom-properties -u postcss-calc -u ./postcss-structure -u postcss-cssnext -i src/styles.css -o dist/styles.css",
    "css-test": "postcss -u postcss-import -u postcss-cssnext -i test/src/styles.css -o test/dist/styles.css",
    "css-structure": "postcss -u postcss-import -u postcss-cssnext -i test/src/structure.css -o test/dist/structure.css",
    "css-01": "postcss --local-plugins -u ./postcss-structure -i test/src/01.css -o test/dist/01.css",
    "css-02": "postcss --local-plugins -u ./postcss-structure -i test/src/02.css -o test/dist/02.css",
    "css-03": "postcss --local-plugins -u ./postcss-structure -i test/src/03.css -o test/dist/03.css",
    "css-04": "postcss --local-plugins -u ./postcss-structure -i test/src/04.css -o test/dist/04.css",
    "css-05": "postcss --local-plugins -u ./postcss-structure -u autoprefixer -i test/src/05.css -o test/dist/05.css",
    "css-06": "postcss --local-plugins -u ./postcss-structure -u autoprefixer -i test/src/06.css -o test/dist/06.css",
    "css-07": "postcss --local-plugins -u ./postcss-structure -i test/src/07.css -o test/dist/07.css",
    "css-08": "postcss --local-plugins -u ./postcss-structure -i test/src/08.css -o test/dist/08.css",
    "css-09": "postcss --local-plugins -u ./postcss-structure -i test/src/09.css -o test/dist/09.css",
    "css-10": "postcss --local-plugins -u ./postcss-structure -i test/src/10.css -o test/dist/10.css",
    "css-11": "postcss --local-plugins -u ./postcss-structure -i test/src/11.css -o test/dist/11.css",
    "css-12": "postcss --local-plugins -u ./postcss-structure -i test/src/12.css -o test/dist/12.css",
    "build": "npm run babel && npm run css",
    "test": "npm run babel && npm run css && npm run css-structure && npm run css-test && npm run css-01 && npm run css-02 && npm run css-03 && npm run css-04 && npm run css-05 && npm run css-06 && npm run css-07 && npm run css-08 && npm run css-09 && npm run css-10 && npm run css-11",
    "watch": "onchange 'index.html' 'src/**/*.*' 'postcss-structure/src/**/*.*' -- npm run build",
    "reload": "livereload",
    "start": "npm-run-all --parallel watch reload"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "livereload": "^0.4.1",
    "npm-run-all": "^1.7.0",
    "onchange": "^2.4.0",
    "postcss": "^5.0.19",
    "postcss-cli": "^2.5.1",
    "postcss-cssnext": "^2.5.2",
    "postcss-import": "^8.1.0",
    "postcss-structure": "0.8.4",
    "structure.css": "^0.6.6"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  }
}
