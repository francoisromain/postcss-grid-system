{
  "name": "postcss-grid-system",
  "version": "0.4.3",
  "description": "A PostCSS plugin to create grids based on a fixed column width.",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "grid",
    "grid-system",
    "flexbox",
    "float",
    "masonry",
    "postcss-structure"
  ],
  "author": "Francois Romain <francoisromain@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/francoisromain/postcss-grid-system.git"
  },
  "bugs": {
    "url": "https://github.com/francoisromain/postcss-grid-system/issues"
  },
  "homepage": "http://francoisromain.github.io/postcss-grid-system/",
  "dependencies": {
    "postcss": "^5.2.12"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "test": "ava && eslint *.js"
  },
  "main": "./lib/index.js",
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.22.0",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1"
  },
  "eslintConfig": {
    "extends": "standard"
  },
  "ava": {
    "files": [
      "test/tests.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
