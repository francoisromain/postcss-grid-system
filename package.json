{
  "name": "postcss-structure",
  "version": "0.7.6",
  "description": "A PostCSS plugin to create CSS grids based on a fixed block width.",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "grid",
    "grid-system",
    "flexbox",
    "float",
    "masonry"
  ],
  "author": "Francois Romain <francoisromain@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/francoisromain/postcss-structure.git"
  },
  "bugs": {
    "url": "https://github.com/francoisromain/postcss-structure/issues"
  },
  "homepage": "https://github.com/francoisromain/postcss-structure",
  "dependencies": {
    "postcss": "^5.0.19"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "test": "ava && eslint *.js"
  },
  "main": "./lib/index.js",
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.3.0"
  },
  "eslintConfig": {
    "extends": "airbnb"
  },
  "ava": {
    "files": [
      "test/tests.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
