{
  "name": "francoisromain.github.io.postcss-structure",
  "version": "0.1.0",
  "scripts": {
    "babel": "babel postcss-structure/src --out-dir postcss-structure/lib",
    "css": "postcss --local-plugins -u postcss-import -u postcss-custom-properties -u postcss-calc -u ./postcss-structure/lib/index.js -u postcss-cssnext -i src/styles.css -o dist/styles.css",
    "css-test": "postcss -u postcss-import -u postcss-cssnext -i test/src/styles.css -o test/dist/styles.css",
    "css-structure": "postcss -u postcss-import -u postcss-cssnext -i test/src/structure.css -o test/dist/structure.css",
    "css-1": "postcss --local-plugins -u ./postcss-structure/lib/index.js -i test/src/grid-1.css -o test/dist/grid-1.css",
    "css-2": "postcss --local-plugins -u ./postcss-structure/lib/index.js -i test/src/grid-2.css -o test/dist/grid-2.css",
    "css-3": "postcss --local-plugins -u ./postcss-structure/lib/index.js -i test/src/grid-3.css -o test/dist/grid-3.css",
    "css-4": "postcss --local-plugins -u ./postcss-structure/lib/index.js -i test/src/grid-4.css -o test/dist/grid-4.css",
    "css-5": "postcss --local-plugins -u ./postcss-structure/lib/index.js -u autoprefixer -i test/src/grid-5.css -o test/dist/grid-5.css",
    "css-6": "postcss --local-plugins -u ./postcss-structure/lib/index.js -u autoprefixer -i test/src/grid-6.css -o test/dist/grid-6.css",
    "css-7": "postcss --local-plugins -u ./postcss-structure/lib/index.js -i test/src/grid-7.css -o test/dist/grid-7.css",
    "css-8": "postcss --local-plugins -u ./postcss-structure/lib/index.js -i test/src/grid-8.css -o test/dist/grid-8.css",
    "build": "npm run babel && npm run css",
    "build-test": "npm run babel && npm run css && npm run css-structure && npm run css-test && npm run css-1 && npm run css-2 && npm run css-3 && npm run css-4 && npm run css-5 && npm run css-6 && npm run css-7 && npm run css-8",
    "watch": "onchange 'index.html' 'src/**/*.*' 'postcss-structure/src/**/*.*' -- npm run build",
    "reload": "livereload",
    "start": "npm-run-all --parallel watch reload"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "livereload": "^0.4.1",
    "npm-run-all": "^1.7.0",
    "onchange": "^2.3.0",
    "postcss": "^5.0.19",
    "postcss-cli": "^2.5.1",
    "postcss-cssnext": "^2.5.2",
    "postcss-import": "^8.1.0",
    "postcss-structure": "0.5.4",
    "structure.css": "^0.4.4"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
